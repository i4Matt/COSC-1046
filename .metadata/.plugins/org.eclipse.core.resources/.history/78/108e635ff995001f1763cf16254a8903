package Testing;

public class Methods {
	
	public static int max(int num[]) {
		int highest = 0;
		
		for (int i = 0; i < num.length; i++) {
			if(num[i] > highest) {
				highest = num[i];
			}
		}
		
		return highest;
	}
	
	public static void main(String[] args) {
		int test[] = {7,3,6,2,1,6,7,};
		System.out.print(max(test));

	}

}
