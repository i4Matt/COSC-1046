package Testing;

public class Methods {
	
	public static int max(int num[]) {
		int highest = 0;
		
		for (int i = 0; i < num.length; i++) {
			if(num[i] > highest) {
				highest = num[i];
			}
		}
		
		return highest;
		
	}
	
	public static void main(String[] args) {
		System.out.print(max(1,7));
		
		
		int test[] = {1, 3};

	}

}
