package Testing;

public class TeacherSaidSo {
	
	public static int sumNatural(int count) {
		int total = 0;
		for (int i = 0; i < count; i++) {
			total = total + i + 1;
		}
		
		return total;
	}
	public static void main(String[] args) {
		int nat10 = sumNatural(10);
		
		System.out.print(nat10);

	}

}
