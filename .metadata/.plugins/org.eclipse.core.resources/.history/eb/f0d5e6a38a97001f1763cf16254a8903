package Testing;

public class Methods {
	

	
	public static void main(String[] args) {
		int test[] = {75,31,46,26,112,63,27};
		System.out.print(max(test));

	}
	
	public static int max(int num[]) {
		int highest = 0;
		
		for (int i = 0; i < num.length; i++) {
			if(num[i] > highest) {
				highest = num[i];
			}
		}
		
		return highest;
	}

}
